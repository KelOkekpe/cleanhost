<table class = "table">
  <thead>
  <tr>
    <th scope='col'>#</th>
    <th scope='col'>id</th>
    <th scope='col'>name</th>
    <th scope='col'>email</th>
    <th scope='col'>created_at</th>
    <th scope='col'>trial_paused</th>
  </tr>
  <%= count = 1 %>
<%= @users.each do |user| %>
  <tr>
    <th scope="row"><%= count %></th>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
    <td><%= user.trial_paused %></td>
    <%= count += 1 %>
<% end %>
</table>
